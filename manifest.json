{
    "manifest_version" : 3,
    "name": "Wikipedia Reading Time",
    "description": "Displays the reading time of Wikipedia articles.",
    "version": "1.0",
    "icons": {
        "128": "icon-500.png"   
    },
    "content_scripts" : [
        {
            "js": ["content.js"],
            "matches": [
                "https://en.wikipedia.org/wiki/*",
                "https://en.m.wikipedia.org/wiki/*"

            ]
        }
    ]
}